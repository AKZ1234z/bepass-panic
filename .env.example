# Bepass Panic Control - Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env file with real credentials!

# Security
PANIC_HASH=your_panic_hash_here
LOGIN_USERNAME=bepass@bepass.com.br
LOGIN_PASSWORD=your_password_here

# SSH Configuration
SSH_USER=bepass
SSH_PORT=22
SSH_KEY_PATH=~/.ssh/your_key_name

# API Configuration
BEBOX_API_PORT=3005
APP_PORT=3456

# Server
BEPASS_SERVER_IP=10.102.0.x
